{
  "name": "Tweet Information Gatherer",
  "version": "0.1",
  "manifest_version": 3,
  "icons": {
    "128": "assets/img/icon.png"
  },
  "background": {
    "service_worker": "assets/js/background.js"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}